func main () int32 {
    a := "Hello, World!";
    ab := sum(56782, 42839);
    ab2 := times(ab, 7);

    if 6 == 6 {
        println("If block");
    } else {
        println("Else block");
    }

    println(a);
    println(ab2);
    return 0;
}

func sum(a: int32, b: int32) int32 {
    return a + b;
}

func times(a: int32, b: int32) int32 {
    return a * b;
}