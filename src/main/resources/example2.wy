func main () {
    a := "Hello, World!";
    ab := sum(56782, 42839);
    ab2 := times(ab, 7);

    if false {
        println("If block");
    } else {
        println("Else block");
    }

    println(a);
    println(ab2);
}

func sum(a: int32, b: int32) int32 {
    return a + b;
}

func times(a: int32, b: int32) int32 {
    return a * b;
}